<div class="digg_pagination">
  <%= will_paginate @microposts %>
</div>

<% @photos.each do |photo| %>
    <div class="col-xs-6 col-md-3">
      <div class="photo-block">
        <div class="photo-image">
          <%= link_to (image_tag image_path photo.image.url(:medium)), photo %>
        </div>

        <caption>
          <h3 style="white-space: nowrap;overflow: hidden;"><%= photo.title %></h3>
          <p>By: <%= link_to photo.user.name, show_user_path(photo.user) %></p>
        </caption>
      </div>
    </div>
<% end %>


